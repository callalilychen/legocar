#!/usr/bin/gnuplot

set terminal push
set terminal postscript eps enhanced color blacktext
set output "eich35.eps"
#set title "bla"
set xlabel "time [s]"
set ylabel "value"
set grid mxtics x
set grid mytics y
#set logscale y
#set logscale x
eich = "ourlog3"


set yrange [0.08:1]
#set xrange [62:66]
set xrange [162:215]
set yrange [0:15]
set samples 1000

plot eich using ( $0/10 ):($1*10*3.6/80) title "front" with linespoints, eich using ($0/10):($2*10*3.6/80) title "back" with linespoints, eich using ($0/10):($3*10*3.6/80) title "desired" with linespoints,eich using  ($0/10):(($4-4000)/4000*10) title "engine current" with linespoints




